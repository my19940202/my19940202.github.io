  <SchoolBrief>
    <fBrief>&lt;style&gt;&lt;font style="font-family: 宋体;"&gt;&amp;lt;font style="color: rgb(128, 128, 128); font-family: 黑体; font-size: 10pt;"&amp;gt;&amp;amp;lt;font style="font-size: 11pt;"&amp;amp;gt;
/* Generator: eWebEditor */
